{
  "name": "news-feed-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:backend": "npm --workspace=@news-feed-app/backend run start:dev",
    "dev:frontend": "npm --workspace=@news-feed-app/frontend run dev",
    "dev": "npx concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:backend": "npm --workspace=@news-feed-app/backend run build",
    "build:frontend": "npm --workspace=@news-feed-app/frontend run build",
    "build": "npx concurrently \"npm run build:backend\" \"npm run build:frontend\"",
    "test:backend": "npm --workspace=@news-feed-app/backend run test",
    "test": "npx concurrently \"npm run test:backend\"",
    "lint:backend": "npm --workspace=@news-feed-app/backend run lint",
    "lint:frontend": "npm --workspace=@news-feed-app/frontend run lint",
    "lint": "npx concurrently \"npm run lint:backend\" \"npm run lint:frontend\"",
    "install:backend": "npm --workspace=@news-feed-app/backend install",
    "install:frontend": "npm --workspace=@news-feed-app/frontend install",
    "install-deps": "npx concurrently \"npm run install:backend\" \"npm run install:frontend\"",
    "migrate": "npm --workspace=@news-feed-app/backend run migration:run"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}